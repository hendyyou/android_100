package cn.digione.yibaic.shop.acitvity;import android.os.Bundle;import android.support.v4.app.FragmentManager;import android.view.MenuItem;import android.view.Window;import cn.digione.yibaic.shop.R;import cn.digione.yibaic.shop.ui.RegisterSelectAccountTypeFragment;/** * Created with IntelliJ IDEA. * User: youzh * Date: 14-3-4 * Time: 下午2:57 */public class RegisterAccountActivity extends BaseActivity {    private FragmentManager mFragmentManager;    public boolean onOptionsItemSelected(MenuItem menuitem) {        return super.onOptionsItemSelected(menuitem);    }    public void onCreate(Bundle bundle) {        super.onCreate(bundle);        setCustomContentView(R.layout.register_activity);        setTitle(R.string.title_reg);        mFragmentManager = getSupportFragmentManager();        RegisterSelectAccountTypeFragment registerselectaccounttypefragment = new RegisterSelectAccountTypeFragment();        mFragmentManager.beginTransaction().add(R.id.fragment_container, registerselectaccounttypefragment)                        .commitAllowingStateLoss();        setShoppingBarEnable(false);    }}