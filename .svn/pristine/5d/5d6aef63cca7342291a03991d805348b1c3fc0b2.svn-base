<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/white" >

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:id="@+id/account_info_container"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="9.0dip"
                android:paddingLeft="10.0dip"
                android:paddingRight="10.0dip"
                android:paddingTop="10.0dip" >

                <RelativeLayout
                    android:id="@+id/account_login_layout"
                    style="@style/Widget.Item.ListView.MutiLine"
                    android:background="@drawable/service_item_text_bg" >

                    <ImageView
                        android:id="@+id/login_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15.0dip"
                        android:adjustViewBounds="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:src="@drawable/search_order_icon" />

                    <TextView
                        android:id="@+id/login_text"
                        style="@style/TextAppearance.List.Primary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/login_icon"
                        android:text="@string/login"
                        android:textSize="@dimen/text_size_medium" />

                    <ImageView
                        android:id="@+id/login_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:scaleType="centerInside"
                        android:src="@drawable/btn_next" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/account_logout_layout"
                    style="@style/Widget.Item.ListView.MutiLine"
                    android:background="@drawable/service_item_text_bg"
                    android:visibility="gone" >

                    <ImageView
                        android:id="@+id/logout_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15.0dip"
                        android:adjustViewBounds="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:src="@drawable/search_order_icon" />

                    <TextView
                        android:id="@+id/logout_text"
                        style="@style/TextAppearance.List.Primary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/logout_icon"
                        android:text="@string/logout"
                        android:textSize="@dimen/text_size_medium" />

                    <ImageView
                        android:id="@+id/logout_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:scaleType="centerInside"
                        android:src="@drawable/btn_next" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="9.0dip"
                android:paddingLeft="10.0dip"
                android:paddingRight="10.0dip" >

                <RelativeLayout
                    android:id="@+id/order_list"
                    style="@style/Widget.Item.ListView.MutiLine"
                    android:background="@drawable/service_item_text_bg" >

                    <ImageView
                        android:id="@+id/order_item_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15.0dip"
                        android:adjustViewBounds="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:src="@drawable/search_order_icon" />

                    <TextView
                        android:id="@+id/order_item_text"
                        style="@style/TextAppearance.List.Primary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/order_item_icon"
                        android:text="@string/account_my_order_list"
                        android:textSize="@dimen/text_size_medium" />

                    <ImageView
                        android:id="@+id/order_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:scaleType="centerInside"
                        android:src="@drawable/btn_next" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="9.0dip"
                android:paddingLeft="10.0dip"
                android:paddingRight="10.0dip" >

                <RelativeLayout
                    android:id="@+id/book_list"
                    style="@style/Widget.Item.ListView.MutiLine"
                    android:background="@drawable/service_item_text_bg" >

                    <ImageView
                        android:id="@+id/book_item_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15.0dip"
                        android:adjustViewBounds="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:src="@drawable/search_order_icon" />

                    <TextView
                        android:id="@+id/am_item_text"
                        style="@style/TextAppearance.List.Primary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@id/book_item_icon"
                        android:text="@string/book_query"
                        android:textSize="@dimen/text_size_medium" />

                    <ImageView
                        android:id="@+id/book_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:clickable="false"
                        android:focusable="false"
                        android:scaleType="centerInside"
                        android:src="@drawable/btn_next" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="9.0dip"
                android:paddingLeft="10.0dip"
                android:paddingRight="10.0dip" >

                <RelativeLayout
                    android:id="@+id/mycoupons_layout"
                    style="@style/Widget.Item.ListView.MutiLine"
                    android:background="@drawable/service_item_text_bg" >

                    <ImageView
