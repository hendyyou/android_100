package cn.digione.yibaic.shop.acitvity;import android.os.Bundle;import android.support.v4.app.FragmentManager;import android.view.MenuItem;import cn.digione.yibaic.shop.R;import cn.digione.yibaic.shop.common.Constants;import cn.digione.yibaic.shop.ui.RegisterSelectAccountTypeFragment;/** * Created with IntelliJ IDEA. * User: youzh * Date: 14-3-4 * Time: 下午2:57 */public class RegisterAccountActivity extends BaseActivity {    public boolean onOptionsItemSelected(MenuItem menuitem) {        return super.onOptionsItemSelected(menuitem);    }    public void onCreate(Bundle bundle) {        super.onCreate(bundle);        setCustomContentView(R.layout.register_activity);        Bundle argBundle = getIntent().getExtras();        if (null != argBundle) {            setTitle(argBundle.getInt(Constants.ArgName.UI.RegisterAccountActivity.TITLE_NAME));            FragmentManager mFragmentManager = getSupportFragmentManager();            RegisterSelectAccountTypeFragment registerselectaccounttypefragment = new RegisterSelectAccountTypeFragment();            registerselectaccounttypefragment.setArguments(argBundle);            mFragmentManager.beginTransaction().add(R.id.fragment_container, registerselectaccounttypefragment)                            .commitAllowingStateLoss();            setShoppingBarEnable(false);        }    }}