package cn.digione.yibaic.shop.acitvity;import android.content.Intent;import android.os.Bundle;import android.support.v4.app.Fragment;import cn.digione.yibaic.shop.R;import cn.digione.yibaic.shop.ui.FMaBuyFragment;import cn.digione.yibaic.shop.ui.ProductColorFragment;import cn.digione.yibaic.shop.ui.ProductDetailsFragment;import cn.digione.yibaic.shop.ui.ProductListFragment;import cn.digione.yibaic.shop.util.Log;/** * Created with IntelliJ IDEA. User: youzh Date: 14-3-4 Time: 上午10:46 */public class ProductListActivity extends BaseActivity {	@Override	protected Fragment newFragmentByTag(String s) {		Fragment fragment = null;		if ("product_list_fragment".equals(s)) {			fragment = new ProductListFragment();		}		if ("product_details_fragment".equals(s)) {			fragment = new ProductDetailsFragment();		}		if ("color_select_fragment".equals(s)) {			fragment = new ProductColorFragment();		}		if ("fmabuy_fragment".equals(s)) {			fragment = new FMaBuyFragment();		}		return fragment;	}	@Override	protected void onCreate(Bundle bundle) {		super.onCreate(bundle);		setHomeButtonEnable(true);		setCustomContentView(R.layout.product_activity);		// mFragmentManager = getSupportFragmentManager();		// 专题页和广告页跳转过来，直接进到产品选择列表。		if ("index_select_product".equals(getIntent().getAction())) {			showFragment("color_select_fragment", getIntent().getExtras(), false);			return;		} else if ("index_detail_product".equals(getIntent().getAction())) {			showFragment("product_details_fragment", getIntent().getExtras(), false);			return;		}		Log.d("ProductListActivity------onCreate()");		showFragment("product_list_fragment", getIntent().getExtras(), false);		/*		 * mFragmentManager = getSupportFragmentManager(); mProductListFragment = new ProductListFragment();		 * mFragmentManager.beginTransaction().add(R.id.fragment_container, mProductListFragment).commit();		 */	}	@Override	protected void onNewIntent(final Intent intent) {		super.onNewIntent(intent);		Log.d("ProductListActivity------onNewIntent()");		showFragment("product_list_fragment", intent.getExtras(), false);	}}